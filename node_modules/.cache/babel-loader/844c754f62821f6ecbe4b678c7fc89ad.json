MADA,EACA;AAAA;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AACA,oBAAA,CAAA,KAAA,CAAA,MAAA,EAAA;AACA,kBAAA,KAAA,CAAA,OAAA,GAAA,IAAA;AACA;;AAHA;AAAA,uBAKA,YAAA,CAAA,MAAA,CAAA,QAAA,EAAA;AACA,kBAAA,IAAA,EAAA,KAAA,CAAA,IADA;AAEA,kBAAA,IAAA,EAAA;AAFA,iBAAA,EAIA,KAJA,CAIA,UAAA,GAAA,EAAA;AACA,kBAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,kBAAA,KAAA,CAAA,OAAA,GAAA,IAAA;AACA,iBAPA,EAQA,OARA,CAQA,YAAA;AACA,kBAAA,KAAA,CAAA,OAAA,GAAA,KAAA;AACA,iBAVA,CALA;;AAAA;AAKA,gBAAA,MALA;;AAAA,sBAiBA,OAAA,MAAA,KAAA,WAjBA;AAAA;AAAA;AAAA;;AAAA;;AAAA;AAmBA,oBAAA,MAAA,CAAA,IAAA,CAAA,IAAA,CAAA,MAAA,EAAA;AACA,kBAAA,KAAA,CAAA,IAAA,IAAA,CAAA;;AACA,kCAAA,KAAA,CAAA,MAAA,EAAA,IAAA,wCAAA,MAAA,CAAA,IAAA,CAAA,IAAA;;AACA,kBAAA,MAAA,CAAA,MAAA;AACA,kBAAA,KAAA,CAAA,MAAA,GAAA,IAAA;AACA,iBALA,MAKA;AACA,kBAAA,MAAA,CAAA,QAAA;AACA;;AA1BA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AA2BA,KA5BA;AA6BA,IAAA,YA7BA,0BA6BA;AAAA,UAAA,MAAA,uEAAA,EAAA;AAAA,UAAA,GAAA;;AACA,UAAA,MAAA,CAAA,MAAA,IAAA,GAAA,EAAA;AACA,eAAA,MAAA;AACA;;AACA,aAAA,MAAA,CAAA,KAAA,CAAA,CAAA,EAAA,GAAA,CAAA;AACA,KAlCA;AAmCA,IAAA,aAnCA,yBAmCA,IAnCA,EAmCA;AACA,aAAA,MAAA,CAAA,IAAA,CAAA,CAAA,OAAA,EAAA;AACA;AArCA,GAZA;AAmDA,EAAA,UAAA,EAAA;AACA,IAAA,eAAA,EAAA;AADA;AAnDA,CAAA","sourcesContent":["<template>\r\n  <div id=\"trending\" class=\"pt-7 px-sm-10\">\r\n    <v-container fluid>\r\n      <v-row>\r\n        <v-col\r\n          cols=\"8\"\r\n          sm=\"7\"\r\n          md=\"10\"\r\n          lg=\"10\"\r\n          v-for=\"(video, i) in loading ? 12 : videos\"\r\n          :key=\"i\"\r\n          class=\"mx-lg-0 mx-md-0 mx-sm-auto mx-auto\"\r\n        >\r\n          <v-skeleton-loader\r\n            class=\"mx-auto\"\r\n            type=\"list-item-avatar-three-line\"\r\n            :loading=\"loading\"\r\n            tile\r\n            large\r\n          >\r\n            <v-card class=\"card\" tile flat :to=\"`/watch/${video._id}`\">\r\n              <v-row no-gutters>\r\n                <v-col class=\"mx-auto\" cols=\"12\" sm=\"8\" md=\"5\" lg=\"4\">\r\n                  <!-- <v-responsive max-height=\"100%\"> -->\r\n                  <v-img\r\n                    max-height=\"200\"\r\n                    class=\"align-center\"\r\n                    :src=\"`${getUrl}/uploads/thumbnails/${video.thumbnailUrl}`\"\r\n                  >\r\n                  </v-img>\r\n                  <!-- </v-responsive> -->\r\n                </v-col>\r\n                <v-col class=\"hidden-sm-and-down\">\r\n                  <div class=\"ml-4 \">\r\n                    <v-card-title class=\"pl-2 pt-0 subtitle-1 font-weight-bold\">\r\n                      {{ video.title }}\r\n                    </v-card-title>\r\n\r\n                    <v-card-subtitle class=\"pl-2 pb-0\" v-if=\"video.userId\">\r\n                      {{ video.userId.channelName }}\r\n                      <v-icon>mdi-circle-small</v-icon\r\n                      >{{ video.views }} views<v-icon>mdi-circle-small</v-icon\r\n                      >{{ dateFormatter(video.createdAt) }}\r\n                    </v-card-subtitle>\r\n                    <v-card-subtitle class=\"pl-2 pt-0\">\r\n                      {{ truncateText(video.description, 200) }}\r\n                    </v-card-subtitle>\r\n                  </div>\r\n                </v-col>\r\n              </v-row>\r\n            </v-card>\r\n          </v-skeleton-loader>\r\n        </v-col>\r\n        <v-col class=\"text-center\" v-if=\"videos.length === 0 && !loading\">\r\n          <p>No trending videos yet</p>\r\n        </v-col>\r\n        <v-col cols=\"12\" sm=\"12\" md=\"12\" lg=\"12\">\r\n          <infinite-loading @infinite=\"getVideos\">\r\n            <div slot=\"spinner\">\r\n              <v-progress-circular\r\n                indeterminate\r\n                color=\"red\"\r\n              ></v-progress-circular>\r\n            </div>\r\n            <div slot=\"no-results\"></div>\r\n            <span slot=\"no-more\"></span>\r\n            <div slot=\"error\" slot-scope=\"{ trigger }\">\r\n              <v-alert prominent type=\"error\">\r\n                <v-row align=\"center\">\r\n                  <v-col class=\"grow\">\r\n                    <div class=\"title\">Error!</div>\r\n                    <div>\r\n                      Something went wrong, but don’t fret — let’s give it\r\n                      another shot.\r\n                    </div>\r\n                  </v-col>\r\n                  <v-col class=\"shrink\">\r\n                    <v-btn @click=\"trigger\">Take action</v-btn>\r\n                  </v-col>\r\n                </v-row>\r\n              </v-alert>\r\n            </div>\r\n          </infinite-loading>\r\n        </v-col>\r\n      </v-row>\r\n    </v-container>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport InfiniteLoading from 'vue-infinite-loading'\r\nimport moment from 'moment'\r\n\r\nimport { mapGetters } from 'vuex'\r\n\r\nimport VideoService from '@/services/VideoService'\r\n\r\nexport default {\r\n  name: 'Trending',\r\n  data: () => ({\r\n    loading: false,\r\n    loaded: false,\r\n    errored: false,\r\n    videos: [],\r\n    page: 1\r\n  }),\r\n  computed: {\r\n    ...mapGetters(['currentUser', 'getUrl', 'isAuthenticated'])\r\n  },\r\n  methods: {\r\n    async getVideos($state) {\r\n      if (!this.loaded) {\r\n        this.loading = true\r\n      }\r\n\r\n      const videos = await VideoService.getAll('public', {\r\n        page: this.page,\r\n        sort: '-views'\r\n      })\r\n        .catch((err) => {\r\n          console.log(err)\r\n          this.errored = true\r\n        })\r\n        .finally(() => {\r\n          this.loading = false\r\n        })\r\n\r\n      if (typeof videos === 'undefined') return\r\n\r\n      if (videos.data.data.length) {\r\n        this.page += 1\r\n        this.videos.push(...videos.data.data)\r\n        $state.loaded()\r\n        this.loaded = true\r\n      } else {\r\n        $state.complete()\r\n      }\r\n    },\r\n    truncateText(string = '', num) {\r\n      if (string.length <= num) {\r\n        return string\r\n      }\r\n      return string.slice(0, num)\r\n    },\r\n    dateFormatter(date) {\r\n      return moment(date).fromNow()\r\n    }\r\n  },\r\n  components: {\r\n    InfiniteLoading\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\">\r\n.v-skeleton-loader__list-item-avatar-three-line {\r\n  height: 250px;\r\n  .v-skeleton-loader__paragraph.v-skeleton-loader__bone {\r\n    margin-top: -125px;\r\n    @media (max-width: 700px) {\r\n      display: none;\r\n      margin-top: inherit;\r\n    }\r\n  }\r\n  .v-skeleton-loader__avatar {\r\n    height: 200px !important;\r\n    width: 300px !important;\r\n  }\r\n  .v-skeleton-loader__text {\r\n    height: 20px;\r\n  }\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views"}]}